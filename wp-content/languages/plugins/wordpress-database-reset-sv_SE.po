msgid "Enter the security code into the text box"
msgstr "Ange säkerhetskoden i textrutan"

#: lib/helpers.php:119
msgid "User already exists. Password inherited."
msgstr "Användaren finns redan. Lösenordet ärvdes."

#: lib/helpers.php:111
msgid "<strong><em>Note that password</em></strong> carefully! It is a <em>random</em> password that was generated just for you."
msgstr "<strong><em>Notera det lösenordet</em></strong> noggrant! Det är ett <em>slumpmässigt</em> lösenord som genererades just för dig."

#: views/partials/disclaimer.php:2
msgid "You selected the users table. Only the <strong><u>%s</u></strong> user will be restored"
msgstr "Du valde användartabellen. Endast användaren <strong><u>%s</u></strong> kommer att återställas"

#: class-db-reset-admin.php:98
msgid "You entered the wrong security code"
msgstr "Du angav fel säkerhetskod"

#: class-db-reset-admin.php:79 class-db-reset-command.php:92
