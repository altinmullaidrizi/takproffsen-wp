#: simple-google-recaptcha.php:326
msgid "Notice"
msgstr "Aviso"

#: simple-google-recaptcha.php:278
msgid "The response is no longer valid: either is too old or has been used previously."
msgstr "La respuesta ya no es válida: es muy antigua o ha sido usada anteriormente."

#: simple-google-recaptcha.php:276
msgid "The request is invalid or malformed."
msgstr "La solicitud no es válida o está mal formada."

#: simple-google-recaptcha.php:274
msgid "The response parameter is invalid or malformed."
msgstr "El parámetro de respuesta no es válido o está mal formado."

#: simple-google-recaptcha.php:272
msgid "The secret parameter is invalid or malformed."
msgstr "El parámetro secreto no es válido o está mal formado."

#: simple-google-recaptcha.php:270
msgid "The response parameter is missing."
msgstr "Falta el parámetro de respuesta."

#: simple-google-recaptcha.php:268
msgid "The secret parameter is missing."
msgstr "Falta el parámetro secreto."

#: simple-google-recaptcha.php:332
msgid "Warning"
msgstr "Advertencia"

#. Description of the plugin
msgid "Simply protect your WordPress against spam comments and brute-force attacks, thanks to Google reCAPTCHA!"
msgstr "¡Simplemente protege tu WordPress contra spam en los comentarios y ataques de fuerza bruta, gracias a Google reCAPTCHA!"

